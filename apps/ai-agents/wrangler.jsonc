{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ai-agents",
  "main": "src/index.ts",
  "compatibility_date": "2025-10-26",
  "compatibility_flags": ["nodejs_compat"],
  "dev": {
    "port": 8789,
    "inspector_port": 9231,
  },
  "observability": {
    "enabled": true,
  },
  // Environment variables will be set as secrets via wrangler
  // Required secrets:
  // - OPENAI_API_KEY
  // - ELEVENLABS_API_KEY
  // - ELEVENLABS_AGENT_ID
  // - ELEVENLABS_PHONE_NUMBER_ID
  // - SCHOOL_NAME (optional, defaults to "Colegio Skyward")
}
